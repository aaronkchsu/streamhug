    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "better-queue": "^3.8.10",
    "boom": "^7.2.0",
    "color": "^3.0.0",
    "commander": "^2.15.0",
    "dotenv": "^8.0.0",
    "findorcreate-promise": "1.2.0",
    "gql": "^1.1.2",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "hapi": "^17.2.2",
    "inert": "^5.1.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.2.0",
